{
    "family": "dca-task",
    "taskRoleArn": "arn:aws:iam::496513254117:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::496513254117:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "dca-container",
            "image": "496513254117.dkr.ecr.us-west-2.amazonaws.com/dca-visionias",
            "cpu": 256,
            "memory": 1024,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "redis-container"
                },
                {
                    "name": "REDIS_PASSWORD",
                    "value": "visionias=12w/slalKSaKA-=1@sj"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dca-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8000/ || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            }
        },
        {
            "name": "redis-container",
            "image": "redis:latest",
            "cpu": 128,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "command": [
                "redis-server",
                "--requirepass",
                "visionias=12w/slalKSaKA-=1@sj"
            ],
            "environment": [
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                },
                {
                    "name": "REDIS_PASSWORD",
                    "value": "visionias=12w/slalKSaKA-=1@sj"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dca-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "512",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
