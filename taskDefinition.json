{
    "family": "dca-task",
    "taskRoleArn": "arn:aws:iam::496513254117:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::496513254117:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "dca-container",
            "image": "496513254117.dkr.ecr.us-west-2.amazonaws.com/dca-visionias",
            "cpu": 512,
            "memory": 2048,
            "links": [
                "redis-container"
            ],
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "redis-container"
                },
                {
                    "name": "FRANKENPHP_NO_TLS",
                    "value": "1"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dca-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8000/ || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            },
            "systemControls": []
        },
        {
            "name": "redis-container",
            "image": "redis:latest",
            "cpu": 128,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dca-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        },
        {
            "name": "nginx-container",
            "image": "496513254117.dkr.ecr.us-west-2.amazonaws.com/dca-proxy:latest",
            "cpu": 256,
            "memory": 512,
            "links": [
                "dca-container"
            ],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [
                {
                    "sourceVolume": "nginx-config",
                    "containerPath": "/etc/nginx/conf.d"
                }
            ],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/dca-task",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "volumes": [
        {
            "name": "nginx-config",
            "host": {
                "sourcePath": "/ecs/nginx/nginx.conf"
            }
        }
    ],
    "cpu": "1024",
    "memory": "3072",
    "requiresCompatibilities": [
        "EC2"
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
